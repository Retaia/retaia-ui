{
  "name": "retaia-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:architecture": "eslint src --ext .ts,.tsx",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:a11y": "vitest run src/a11y.test.tsx",
    "test:coverage": "vitest run --coverage",
    "i18n:check": "tsx scripts/check-i18n-keys.ts",
    "bdd:test": "cucumber-js \"bdd/features/**/*.feature\" --tags \"not @real-api\" --require-module tsx/cjs --require \"bdd/step-definitions/**/*.ts\" --format progress",
    "bdd:test:ci": "cucumber-js \"bdd/features/**/*.feature\" --tags \"not @real-api\" --require-module tsx/cjs --require \"bdd/step-definitions/**/*.ts\" --format progress --format json:test-results/bdd-report.json --format html:test-results/bdd-report.html",
    "bdd:test:real-api": "BDD_API_MODE=real-api cucumber-js \"bdd/features/**/*.feature\" --tags \"@real-api\" --require-module tsx/cjs --require \"bdd/step-definitions/**/*.ts\" --format progress",
    "bdd:test:real-api:ci": "BDD_API_MODE=real-api cucumber-js \"bdd/features/**/*.feature\" --tags \"@real-api\" --require-module tsx/cjs --require \"bdd/step-definitions/**/*.ts\" --format progress --format json:test-results/bdd-real-api-report.json --format html:test-results/bdd-real-api-report.html",
    "bdd:test:critical": "cucumber-js \"bdd/features/**/*.feature\" --require-module tsx/cjs --require \"bdd/step-definitions/**/*.ts\" --tags \"@critical\" --format progress",
    "bdd:test:critical:ci": "cucumber-js \"bdd/features/**/*.feature\" --require-module tsx/cjs --require \"bdd/step-definitions/**/*.ts\" --tags \"@critical\" --format progress --format json:test-results/bdd-critical-report.json --format html:test-results/bdd-critical-report.html",
    "bdd:test:headed": "PW_HEADLESS=false cucumber-js \"bdd/features/**/*.feature\" --require-module tsx/cjs --require \"bdd/step-definitions/**/*.ts\" --format progress",
    "bdd:coverage:run": "rm -rf coverage/bdd && BDD_COVERAGE=1 BDD_COVERAGE_DIR=coverage/bdd/raw node scripts/ci/run-bdd-e2e.mjs --script bdd:test",
    "bdd:coverage:check": "BDD_COVERAGE_METRICS=lines,functions,branches node scripts/ci/check-bdd-browser-coverage.mjs",
    "bdd:coverage": "npm run bdd:coverage:run && npm run bdd:coverage:check",
    "e2e:bdd": "node scripts/ci/run-bdd-e2e.mjs --script bdd:test",
    "e2e:bdd:ci": "node scripts/ci/run-bdd-e2e.mjs --script bdd:test:ci",
    "e2e:bdd:critical": "node scripts/ci/run-bdd-e2e.mjs --script bdd:test:critical",
    "e2e:bdd:critical:ci": "node scripts/ci/run-bdd-e2e.mjs --script bdd:test:critical:ci",
    "preview:ci": "vite preview --host 127.0.0.1 --port 4173",
    "visual:test": "npm run build && start-server-and-test \"npm run preview:ci\" http://127.0.0.1:4173 \"playwright test -c playwright.visual.config.ts\"",
    "visual:test:update": "npm run build && start-server-and-test \"npm run preview:ci\" http://127.0.0.1:4173 \"playwright test -c playwright.visual.config.ts --update-snapshots\"",
    "perf:budget": "node scripts/ci/check-performance-budget.mjs",
    "perf:ci": "npm run build && npm run perf:budget",
    "qa": "npm run i18n:check && npm run lint && npm run test:coverage",
    "qa:coverage:all": "npm run test:coverage && npm run bdd:coverage",
    "prepare": "husky",
    "hooks:install": "husky",
    "typecheck": "tsc -b --pretty false",
    "dup:check": "jscpd src --min-lines 12 --min-tokens 80 --reporters console --format \"ts,tsx\" --threshold 4",
    "runtime:status-driven:check": "node scripts/ci/check-runtime-status-driven.mjs",
    "qa:v1:flows": "npm run runtime:status-driven:check && node scripts/ci/check-v1-critical-flows.mjs",
    "bdd:mock:contract:check": "tsx scripts/ci/check-bdd-mock-openapi-alignment.ts",
    "qa:v1:go-no-go": "node scripts/ci/run-v1-release-gate.mjs",
    "security:audit": "npm audit --omit=dev",
    "gh:protect-master": "node scripts/ci/apply-master-branch-protection.mjs",
    "api:types:generate": "openapi-typescript specs/api/openapi/v1.yaml -o src/api/generated/openapi.ts",
    "api:contract:check": "node scripts/ci/check-api-contract-freeze.mjs",
    "api:contract:freeze": "node scripts/ci/update-api-contract-freeze.mjs",
    "api:governance:check": "node scripts/ci/check-openapi-pr-governance.mjs"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.21",
    "bootstrap": "^5.3.8",
    "bootswatch": "^5.3.8",
    "i18next": "^25.8.5",
    "react": "^19.2.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.4",
    "react-icons": "^5.5.0",
    "react-player": "^2.16.1",
    "react-router-dom": "^7.13.0",
    "sass": "^1.97.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@cucumber/cucumber": "^12.6.0",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "istanbul-lib-coverage": "^3.2.2",
    "jest-axe": "^10.0.0",
    "jscpd": "^4.0.5",
    "jsdom": "^28.0.0",
    "msw": "^2.12.10",
    "openapi-typescript": "^7.12.0",
    "start-server-and-test": "^2.1.3",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "v8-to-istanbul": "^9.3.0",
    "vite": "^8.0.0-beta.13",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "vite": "^8.0.0-beta.13"
  }
}
